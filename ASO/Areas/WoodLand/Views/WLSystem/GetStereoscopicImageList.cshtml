@model List<ASO.Models.StereoscopicImage>

    @if (Model != null)
    {
        foreach (var item in Model)
        {
            <input type="radio" name="sltfile" value="@item.SortID" />
            <img id="StePairL_@item.SortID" src="~/Photos/@Url.Content(item.StePairL)" width="20%" />
            <img id="StePairR_@item.SortID" src="~/Photos/@Url.Content(item.StePairR)" width="20%" />
            <input id="Hidden1" value="@item." type="hidden" />
            <br />
            <br />
        }
    }
    <input type="button" value="像對選取" onclick="returnSteImg()"/>
