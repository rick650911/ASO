@model List<ASO.Models.Species>

    @if (Model != null)
    {
        foreach (var item in Model)
        {
            <div class="PhotoSearch1_contentRIGHT_info_SeparateLine"></div>
            <div class="PhotoSearch1_contentRIGHT_info" style="cursor:pointer;">

                <div class="PhotoSearch1_contentRIGHT_info_Number">
                    <span onclick="OpenData(@item.ID,'@item.ComName')">@item.ID</span>
                </div>
                <div class="PhotoSearch1_contentRIGHT_info_ComName" onclick="OpenData(@item.ID,'@item.ComName')">@item.ComName</div>
                <div class="PhotoSearch1_contentRIGHT_info_IPCCsub" onclick="OpenData(@item.ID,'@item.ComName')">@item.IPCCsub</div>
                <div class="PhotoSearch1_contentRIGHT_info_SciName" onclick="OpenData(@item.ID,'@item.ComName')">@item.Family</div>
                <div class="PhotoSearch1_contentRIGHT_info_Family" onclick="OpenData(@item.ID,'@item.ComName')">
                    <i>@item.SciName</i>
                    @if (!string.IsNullOrEmpty(item.SciSubName))
                    {
                        <br />
                        <span>var.&nbsp;<i>@item.SciSubName</i></span>
                    }
                </div>
            </div>
            <div class="clear"></div>
        }
    }

